{
    "manifest_version": 3,
    "name": "GPT Copilot",
    "version": "1.0.0",
    "action": { "default_popup": "index.html" },
    "host_permissions": [
        "https://api.openai.com/"
      ],
    "content_scripts": [
        {
          "js": ["src/content.js"],
          "matches": ["<all_urls>"]
        }
      ]
}